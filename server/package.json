{
  "name": "aimarketplace-server",
  "version": "1.0.0",
  "description": "Backend API for AIMarketplace - Multi-vendor e-commerce platform",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "node --test",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "keywords": [
    "ecommerce",
    "api",
    "rest",
    "nodejs",
    "express",
    "prisma"
  ],
  "author": "AIMarketplace Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.4.7",
    "morgan": "^1.10.0",
    "compression": "^1.7.5",
    "multer": "^1.4.5-lts.1",
    "stripe": "^17.5.0",
    "nodemailer": "^6.9.16",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "prisma": "^6.5.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2"
  }
}

